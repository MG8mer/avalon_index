entrypoint = "main.py"
modules = ["python-3.10:v18-20230807-322e88b", "python-3.10:v25-20230920-d4ad2e4"]

hidden = [".pythonlibs"]

[nix]
channel = "stable-23_05"
packages = ["ffmpeg-full", "freetype", "lcms2", "libimagequant", "libjpeg", "libopus", "libtiff", "libwebp", "libxcrypt", "openjpeg", "postgresql", "tcl", "tk", "zlib"]

[unitTest]
language = "python3"

[deployment]
run = ["python3", "main.py"]
deploymentTarget = "gce"
ignorePorts = true

[[ports]]
localPort = 8080
externalPort = 80

[languages]

[languages.python3.languageServer]
start = "nice -1"

[agent]
expertMode = true
